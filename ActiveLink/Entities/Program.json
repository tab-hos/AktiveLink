{
    "name": "Program",
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "focus_area": {
        "type": "string",
        "enum": [
          "sports",
          "culture",
          "education",
          "art"
        ],
        "default": "sports"
      },
      "image_url": {
        "type": "string"
      },
      "duration": {
        "type": "string",
        "description": "e.g. '6 weeks', 'Ongoing'"
      }
    },
    "required": [
      "title"
    ]
  }